{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
  //   "noEmit": true,
    "resolveJsonModule": true,
  //   "isolatedModules": true
  },
  "exclude": ["node_modules", "build"]
}
